{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "react/prop-types": [0, { }],
      "no-underscore-dangle": ["error", { "allow": ["_id", "_checkResponse"] }],
      "react/jsx-props-no-spreading": "off",
      "jsx-a11y/label-has-associated-control": "off"
    }
}
